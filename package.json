{
  "name": "multilanguage-server",
  "version": "1.0.0",
  "description": "",
  "bin": "dist/app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=dev nodemon --exec ts-node src/app.ts",
    "start:prod": "cross-env NODE_ENV=prod nodemon --exec ts-node src/app.ts",
    "build": "tsc && tsc-alias",
    "eslint": "eslint --fix .",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "pkg": "pkg . -t linux,win --out-path ./dist",
    "pkg:win": "pkg . -t win --out-path ./dist"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "mysql": "^2.18.1",
    "pkg": "^5.6.0",
    "pkg-fetch": "^3.4.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.5",
    "typeorm-naming-strategies": "^4.1.0",
    "xlsx": "^0.18.5",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "tsc-alias": "^1.6.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "commitlint": "^16.2.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "prettier-eslint": "^13.0.0"
  },
  "_moduleAliases": {
    "~": ".",
    "@": "src",
    "@dao": "src/dao",
    "@router": "src/router",
    "@services": "src/services",
    "@util": "src/util",
    "@middleware": "src/middleware",
    "@exceptions": "src/exceptions"
  }
}
